#include <iostream>
using namespace std;

int main() {
    int N;
    cout << "Введіть розмір масивів N: ";
    cin >> N;

    double *A = new double[N];  // Виділення пам'яті для масиву A
    double *B = new double[N];  // Виділення пам'яті для масиву B

    // Введення елементів масиву A
    cout << "Введіть елементи масиву A:\n";
    for (int i = 0; i < N; i++) {
        cout << "A[" << i << "] = ";
        cin >> A[i];
    }

    // Введення елементів масиву B
    cout << "Введіть елементи масиву B:\n";
    for (int i = 0; i < N; i++) {
        cout << "B[" << i << "] = ";
        cin >> B[i];
    }

    // Заміна додатніх елементів масиву A на відповідні елементи B
    for (int i = 0; i < N; i++) {
        if (A[i] > 0) {  // Якщо елемент додатній
            A[i] = B[i];  // Замінюємо його на відповідний елемент з B
        }
    }

    // Виведення оновленого масиву A
    cout << "Оновлений масив A:\n";
    for (int i = 0; i < N; i++) {
        cout << "A[" << i << "] = " << A[i] << endl;
    }

    // Звільнення пам'яті
    delete[] A;
    delete[] B;

    return 0;
}
